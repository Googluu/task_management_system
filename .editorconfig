# top-most EditorConfig file
root = true

# Use 2 spaces for indentation
[*]
indent_style = space
indent_size = 2

# Use Unix-style line endings
end_of_line = lf

# Charset and encoding
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# Ignore some directories
[**/node_modules/**]
[**/dist/**]
[**/build/**]
